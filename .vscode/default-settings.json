{
    "workbench.colorTheme": "Quiet Light",
    "workbench.iconTheme": "Monokai Pro (Filter Ristretto) Icons",
    "terminal.external.linuxExec": "bash",
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "terminal.integrated.automationShell.linux": "",
    "github-actions.org-features": true,
    "hediet.vscode-drawio.local-storage": "",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "diffEditor.ignoreTrimWhitespace": false,
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"/home/anett/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-16",
            "path": "/usr/lib/jvm/java-16-openjdk-amd64",
            "sources": "/usr/lib/jvm/openjdk-16/src.zip",
            "javadoc": "https://docs.oracle.com/en/java/javase/11/docs/api",
            "default": true
        },
        {
            "name": "JavaSE-11",
            "path": "/usr/lib/jvm/java-1.11.0-openjdk-amd64"
        }
    ],
    "sonarlint.rules": {
        "java:S1854": {
            "level": "off"
        },
        "java:S2864": {
            "level": "off"
        }
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "editor.formatOnSave": true,
    "window.zoomLevel": -2,
    "editor.suggestSelection": "first",
    "editor.renderWhitespace": "all",
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.format.settings.url": ".vscode/java-formatter.xml",
    "java.format.settings.profile": "CodeFormatterProfile",
    "[java]": {
        "editor.defaultFormatter": null,
        "editor.tabSize": 4
    },
    "editor.tabSize": 4,
    "editor.defaultFormatter": "redhat.vscode-commons",
    "editor.autoIndent": "none",
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml",
        "editor.tabSize": 4
    }
}